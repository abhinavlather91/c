
#include <stdio.h>

int main()
{
    char ch="*";
    int rows,columns;
    printf("enter number of rows:");
    
    scanf("%d",&rows);
    printf("enter number of columns:");
    scanf("%d",&columns);
    
    for(int i =1;i<=rows;i++){
        for(int j=1;j<=columns;j++){
            printf("%c",ch);
        }
        printf("\n");
        
    }
    
return 0;
}
