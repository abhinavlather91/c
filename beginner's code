// printing table of any given number

#include <stdio.h>
#include <math.h>


int main()
{
    int n;
    printf("enter number:");
    scanf("%d",&n);
    
    for(int i =1;i<=10;i++){
        printf("%d \n",n*i);
    }
	
	 return 0;
}

//taking input from user until user enters multiple of 7


// Online C compiler to run C program online
#include <stdio.h>
#include <math.h>


int main()
{
    int n;
    
    do{
        printf("enter a number :");
        scanf("%d",&n);
        printf("%d\n",n);
        if(n%7==0){
            break;
        }
    }
        while(1);
        printf("thank you");
  
	
	 return 0;
    
}




