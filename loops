//printing sum in a given range of numbers 
#include<stdio.h>
int main(){
    
    int a ;
    printf ("enter a number:");
    scanf("%d",&a);
    
    int b;
    printf ("enter other number:");
    scanf("%d",&b);
    int sum=0;
    for(int i = a;i<=b;i++){
        sum=sum+i;
    }
    printf("%d",sum);
    return 0;
}
